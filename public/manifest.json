{
  "manifest_version": 3,
  "name": "FAP+",
  "version": "1.0",
  "description": "FPT University Academic Portal Beautifier",
  "permissions": ["activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://fap.fpt.edu.vn/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ]
}
